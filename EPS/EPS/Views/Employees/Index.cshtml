@model IEnumerable<EPS.Models.Employee>

@{
    ViewBag.Title = "Index";
}

<h2>Employee</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MSAlias)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WSAlias)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChineseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EnglishName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateofBirth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Major)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OnBoardDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Group)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lob)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Product)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MSAlias)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WSAlias)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChineseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnglishName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateofBirth.ToShortDateString())
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Major)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OnBoardDate.ToShortDateString())
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OM)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Group)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lob)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Product)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MSAlias }) |
            @Html.ActionLink("Details", "Details", new { id=item.MSAlias }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MSAlias })
        </td>
    </tr>
}

</table>
